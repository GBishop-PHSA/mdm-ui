{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@mdm/*": [
        "./app/*"
      ],
      "@env/*": [
        "./environments/*"
      ],
      "shared": [
        "dist/shared/shared",
        "dist/shared",
        "dist/shared/shared",
        "dist/shared"
      ],
      "mdm-resources/*": [
        "../dist/mdm-resources/mdm-resources",
        "../dist/mdm-resources"
      ],
      "mdm-resources": [
        "../dist/mdm-resources"
      ]
    },
    "emitDecoratorMetadata": true,
    "outDir": "./dist/",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES5",
    "types": [
      "jest"
    ],
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}