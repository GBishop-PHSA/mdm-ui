    },
    "moduleNameMapper": {
      "^@mdm/(.*)$": "<rootDir>/src/app/$1",
      "^@env/(.*)$": "<rootDir>/src/environments/$1"
    },
    "reporters": [
      "default",
      [
        "./node_modules/jest-html-reporter",
