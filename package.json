    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "testResultsProcessor": "<rootDir>/node_modules/jest-junit-reporter",
    "coveragePathIgnorePatterns": [
